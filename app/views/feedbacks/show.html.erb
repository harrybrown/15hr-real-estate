<div class="page-header">
  <%= link_to feedbacks_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_feedback_path(@feedback), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show feedback</h1>
</div>

<dl class="dl-horizontal">
  <dt>User:</dt>
  <dd><%= @feedback.user_id %></dd>

  <dt>Comment:</dt>
  <dd><%= @feedback.comment %></dd>

  <dt>Respond:</dt>
  <dd><%= @feedback.respond %></dd>

</dl>
